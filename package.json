{
  "name": "mixed_beverages",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "build:css": "sass mixed_beverages/static_src/sass/app.sass:mixed_beverages/static/app.css",
    "build:js": "browserify mixed_beverages/static_src/js/app.js -o mixed_beverages/static/app.js -t babelify",
    "build": "npm run build:css && npm run build:js",
    "watch:css": "sass mixed_beverages/static_src/sass/app.sass:mixed_beverages/static/app.css --embed-source-map --watch",
    "watch:js": "watchify mixed_beverages/static_src/js/app.js -o mixed_beverages/static/app.js -t babelify",
    "dev": "concurrently --kill-others 'npm run watch:*'"
  },
  "author": "",
  "license": "Apache",
  "repository": {
    "type": "git",
    "url": "git://github.com/texas/tx_mixed_beverages.git"
  },
  "private": true,
  "devDependencies": {
    "babelify": "^7.3.0",
    "browserify": "^13.3.0",
    "colorbrewer": "^1.0.0",
    "cookies-js": "^1.2.1",
    "d3": "^3.5.5",
    "jquery": "^2.1.3",
    "leaflet": "^0.7.3",
    "leaflet-hash": "^0.2.1",
    "leaflet.markercluster": "^0.5.0",
    "lodash": "^4.15.0",
    "sass": "^1.26.9",
    "standard": "^8.0.0",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "watchify": "^3.11.1"
  }
}
