{
  "name": "mixed_beverages",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "build:css": "sass mixed_beverages/static_src/sass/app.sass:mixed_beverages/static/app.css",
    "build:js": "browserify mixed_beverages/static_src/js/app.js -o mixed_beverages/static/app.js -t babelify",
    "build": "npm run build:css && npm run build:js",
    "watch:css": "sass mixed_beverages/static_src/sass/app.sass:mixed_beverages/static/app.css --embed-source-map --watch",
    "watch:js": "watchify mixed_beverages/static_src/js/app.js -o mixed_beverages/static/app.js -t babelify --debug",
    "django": "./manage.py runserver",
    "dev": "concurrently --kill-others 'npm:watch:*' 'npm:django'"
  },
  "author": "",
  "license": "Apache",
  "repository": {
    "type": "git",
    "url": "git://github.com/texas/tx_mixed_beverages.git"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "babelify": "^10.0.0",
    "browserify": "^16.5.1",
    "colorbrewer": "^1.0.0",
    "concurrently": "^5.2.0",
    "cookies-js": "^1.2.1",
    "d3": "^5.16.0",
    "jquery": "^3.5.1",
    "leaflet": "^1.6.0",
    "leaflet-hash": "^0.2.1",
    "leaflet.markercluster": "^1.4.1",
    "lodash": "^4.17.19",
    "prettier": "^2.0.5",
    "rollup": "^2.21.0",
    "sass": "^1.26.10",
    "watchify": "^3.11.1"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false
  },
  "dependencies": {
    "@condenast/quick-bus": "^0.2.0"
  }
}
